<?xml version="1.0"?>
<Ontology xmlns="http://www.w3.org/2002/07/owl#"
     xml:base="http://www.w3.org/2002/07/owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace">
    <Declaration>
        <ObjectProperty IRI="#contained-in"/>
    </Declaration>
    <Declaration>
        <ObjectProperty IRI="#has-location"/>
    </Declaration>
    <Declaration>
        <ObjectProperty IRI="#part-of"/>
    </Declaration>
    <EquivalentClasses>
        <Class IRI="#Heartdisease"/>
        <ObjectIntersectionOf>
            <Class IRI="#Disease"/>
            <ObjectSomeValuesFrom>
                <ObjectProperty IRI="#has-location"/>
                <Class IRI="#Heart"/>
            </ObjectSomeValuesFrom>
        </ObjectIntersectionOf>
    </EquivalentClasses>
    <SubClassOf>
        <Class IRI="#Endocarditis"/>
        <ObjectIntersectionOf>
            <Class IRI="#Inflammation"/>
            <ObjectSomeValuesFrom>
                <ObjectProperty IRI="#has-location"/>
                <Class IRI="#Endocardium"/>
            </ObjectSomeValuesFrom>
        </ObjectIntersectionOf>
    </SubClassOf>
    <SubClassOf>
        <Class IRI="#Endocardium"/>
        <ObjectIntersectionOf>
            <Class IRI="#Tissue"/>
            <ObjectSomeValuesFrom>
                <ObjectProperty IRI="#contained-in"/>
                <Class IRI="#HeartValve"/>
            </ObjectSomeValuesFrom>
        </ObjectIntersectionOf>
    </SubClassOf>
    <SubClassOf>
        <Class IRI="#HeartValve"/>
        <ObjectIntersectionOf>
            <Class IRI="#BodyValve"/>
            <ObjectSomeValuesFrom>
                <ObjectProperty IRI="#part-of"/>
                <Class IRI="#Heart"/>
            </ObjectSomeValuesFrom>
        </ObjectIntersectionOf>
    </SubClassOf>
    <SubClassOf>
        <Class IRI="#Inflammation"/>
        <ObjectIntersectionOf>
            <Class IRI="#Disease"/>
            <ObjectSomeValuesFrom>
                <ObjectProperty IRI="#acts-on"/>
                <Class IRI="#Tissue"/>
            </ObjectSomeValuesFrom>
        </ObjectIntersectionOf>
    </SubClassOf>
    <SubClassOf>
        <Class IRI="#Pericarditis"/>
        <ObjectIntersectionOf>
            <Class IRI="#Inflammation"/>
            <ObjectSomeValuesFrom>
                <ObjectProperty IRI="#has-location"/>
                <Class IRI="#Pericardium"/>
            </ObjectSomeValuesFrom>
        </ObjectIntersectionOf>
    </SubClassOf>
    <SubClassOf>
        <Class IRI="#Pericardium"/>
        <ObjectIntersectionOf>
            <Class IRI="#Tissue"/>
            <ObjectSomeValuesFrom>
                <ObjectProperty IRI="#contained-in"/>
                <Class IRI="#Heart"/>
            </ObjectSomeValuesFrom>
        </ObjectIntersectionOf>
    </SubClassOf>
    <SubClassOf>
        <ObjectIntersectionOf>
            <Class IRI="#Heartdisease"/>
            <ObjectSomeValuesFrom>
                <ObjectProperty IRI="#has-location"/>
                <Class IRI="#HeartValve"/>
            </ObjectSomeValuesFrom>
        </ObjectIntersectionOf>
        <Class IRI="#IsStateNeedsTreatment"/>
    </SubClassOf>
    <SubObjectPropertyOf>
        <ObjectProperty IRI="#part-of"/>
        <ObjectProperty IRI="#contained-in"/>
    </SubObjectPropertyOf>
    <SubObjectPropertyOf>
        <ObjectPropertyChain>
            <ObjectProperty IRI="#has-location"/>
            <ObjectProperty IRI="#contained-in"/>
        </ObjectPropertyChain>
        <ObjectProperty IRI="#has-location"/>
    </SubObjectPropertyOf>
</Ontology>